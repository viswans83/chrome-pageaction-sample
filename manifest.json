{
  "manifest_version": 2,

  "name": "PageAction Sample",
  "description": "Prefill certain form fields when navigating to a certain page",
  "version": "0.0.1",

  "page_action": {
    "default_icon": {
      "32": "icon32.png"
    }
  },

  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },

  "permissions": [ "activeTab", "tabs" ]
}
